{
    "extends": ["react-app", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y", "react-hooks"],
    "rules": {
        "react/self-closing-comp": [
            "error",
            {
                "component": false,
                "html": false
            }
        ],
        "import/no-anonymous-default-export": ["off"]
    }
}
